{
  "name": "@dwmkerr/ai-providers-and-models",
  "version": "0.1.7",
  "description": "AI providers and models data module for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "cp ../../models.yaml .",
    "build": "npm run update-models && tsc",
    "build:watch": "npm run update-models && tsc -w",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky",
    "prepublishOnly": "npm run build && npm run lint && npm run test",
    "start": "ts-node src/demo.ts",
    "start:debug": "node --inspect-brk node_modules/.bin/ts-node ./src/demo.ts",
    "pretest": "cp ../../models.yaml .",
    "test": "jest --coverage",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --no-coverage",
    "test:watch": "jest --watch --no-coverage",
    "tsc": "tsc",
    "update-models": "cp ../../models.yaml ."
  },
  "files": [
    "dist",
    "models.yaml"
  ],
  "author": "dwmkerr",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "yaml": "^2.7.0"
  }
}
